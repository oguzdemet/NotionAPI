<Window x:Class="NotionAPI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NotionAPI"
        xmlns:WpfApp3="clr-namespace:NotionAPI"
        xmlns:Items="clr-namespace:NotionAPI.Items"
        mc:Ignorable="d"
        Title="NotionAPI" Height="400" Width="800" WindowStyle="SingleBorderWindow">
    <Grid>
        <StackPanel Orientation="Horizontal" Height="35" VerticalAlignment="Top">
            <Button x:Name="MainPageButton" Content="Main Page" Click="Main_Page_Click"/>
            <Button x:Name="ConfigPageButton" Content="Config Page" Click="Config_Page_Click"/>
        </StackPanel>
            <Frame x:Name="Main" Margin="0,35,0,0" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>